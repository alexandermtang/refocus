{
  // Required
  "manifest_version": 2,
  "name": "Refocus",
  "version": "1.0",

  // Recommended

  "description": "Focus on important things",
  "icons": {},


  "background":{
    "scripts": ["bookmark_test.js"]
  },

  "permissions": [
    "bookmarks",
    "history",
    "tabs"
  ],

  "content_scripts": [
   {
    "matches": ["<all_urls>","http://*/*","https://*/*"],
    "js": ["history.js"]
   }
  ]


}
